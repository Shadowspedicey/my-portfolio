{
  "include": [
    "./dist/types/**/*.ts",
    "./next-env.d.ts",
    "./src",
    "build/types/**/*.ts",
    ".next/types/**/*.ts"
  ],
  "exclude": [
    "./node_modules"
  ],
  "compilerOptions": {
    "plugins": [
      {
        "name": "next"
      }
    ],
    "esModuleInterop": true,
    "jsx": "preserve",
    "allowJs": true,
    "forceConsistentCasingInFileNames": true,
    "incremental": true,
    "target": "ES2017",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "skipLibCheck": true,
    "strict": false,
    "noEmit": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "allowImportingTsExtensions": true
  }
}
